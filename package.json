{
  "name": "mvp-combined-prototype",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-navigation/elements": "^1.3.4",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "eslint-plugin-react-native": "^4.0.0",
    "expo": "~45.0.0",
    "expo-file-system": "~14.0.0",
    "expo-notifications": "~0.15.4",
    "expo-permissions": "~13.2.0",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-calendars": "^1.1285.0",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-paper": "^4.12.4",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "0.17.7",
    "unsplash-js": "^7.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react-test-renderer": "^17.0.2"
  },
  "jest": {
    "projects": [
      {
        "displayName": "ios",
        "preset": "jest-expo/ios",
        "setupFiles": [
          "<rootDir>/modules/jest/setup.js",
          "./node_modules/react-native-gesture-handler/jestSetup.js"
        ],
        "moduleNameMapper": {
          "^react-native-reanimated$": "<rootDir>/node_modules/react-native-reanimated/src/Animated.js"
        },
        "transformIgnorePatterns": [
          "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
        ],
        "setupFilesAfterEnv": [
          "@testing-library/jest-native/extend-expect"
        ]
      },
      {
        "preset": "jest-expo/android",
        "setupFiles": [
          "<rootDir>/modules/jest/setup.js",
          "./node_modules/react-native-gesture-handler/jestSetup.js"
        ],
        "moduleNameMapper": {
          "^react-native-reanimated$": "<rootDir>/node_modules/react-native-reanimated/src/Animated.js"
        },
        "transformIgnorePatterns": [
          "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
        ],
        "setupFilesAfterEnv": [
          "@testing-library/jest-native/extend-expect"
        ]
      }
    ]
  },
  "private": true
}
